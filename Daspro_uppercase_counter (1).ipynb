    "# Membuat dan menyimpan modul uppercase_counter.py\n",
    "with open(\"uppercase_counter.py\", \"w\") as file:\n",
    "    file.write(\"\"\"\n",
    "def count_uppercase_letters(text):\n",
    "    count = 0\n",
    "    for char in text:\n",
    "        if char.isupper():\n",
    "            count += 1\n",
    "    return count\n",
    "\"\"\")\n"
   ]
